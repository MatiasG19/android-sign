{
  "name": "android-sign",
  "version": "0.0.0",
  "private": true,
  "description": "Sign apk and aab files.",
  "type": "module",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write ./src",
    "format-check": "prettier --check ./src",
    "lint": "eslint --fix",
    "lint-check": "eslint",
    "package": "ncc build --source-map --license licenses.txt",
    "all": "npm run build && npm run package"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.11.1",
    "@actions/exec": "^1.1.1",
    "@actions/io": "^1.1.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@types/node": "^22.13.10",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.28.0",
    "@vercel/ncc": "^0.38.3",
    "eslint": "^9.23.0",
    "eslint-plugin-github": "^5.1.8",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-prettier": "^5.2.3",
    "js-yaml": "^4.1.0",
    "prettier": "3.5.3",
    "typescript": "^5.8.2"
  }
}
