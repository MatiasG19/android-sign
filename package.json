{
  "name": "android-sign",
  "version": "0.0.0",
  "private": true,
  "description": "Sign apk and aab files.",
  "type": "module",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write ./src",
    "format-check": "prettier --check ./src",
    "lint": "eslint --fix",
    "lint-check": "eslint",
    "package": "ncc build --source-map --license licenses.txt",
    "all": "npm run build && npm run package"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.11.1",
    "@actions/exec": "^1.1.1",
    "@actions/io": "^1.1.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@types/node": "^24.0.10",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.35.1",
    "@vercel/ncc": "^0.38.3",
    "eslint": "^9.30.1",
    "eslint-plugin-github": "^6.0.0",
    "eslint-plugin-prettier": "^5.5.1",
    "js-yaml": "^4.1.0",
    "prettier": "3.6.2",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": "^22",
    "npm": ">= 11"
  }
}
